{
  "Services": {
    "ExchangeRatesApiIOSettings": {
      "BaseUrl": "https://api.apilayer.com",
      "ApiKey": "YOUR_API_KEY"
    }
  },
  "ResiliencyPolicies": {
    "Default": {
      "LifeTime": "5",
      "RetryCounts": "2",
      "SleepDuration": "3",
      "ExponentialSleepDuration": "2",
      "HandledEventsAllowedBeforeBreaking": "5",
      "DurationOfBreak": "30",
      "PessimisticTimeout": "50",
      "OptimisticTimeout": "100"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
